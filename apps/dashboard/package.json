{
  "name": "dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "analyze": "vite-bundle-analyze",
    "build": "yarn prebuild && tsc && vite build",
    "dev": "yarn optimize:svg && yarn makeicons && vite",
    "makeicons": "node ./scripts/transformSvgsIntoComponents.mjs && prettier --write \"src/ui/components/Icon/**/*.{ts,tsx}\"",
    "optimize:svg": "svgo -f ./src/ui/assets/svgs -o ./src/ui/assets/svgs",
    "prebuild": "yarn optimize:svg && yarn optimize:svg && yarn makeicons",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.2",
    "react-router-dom": "^6.6.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vanilla-extract/vite-plugin": "^3.7.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "svgo": "^3.0.2",
    "tsconfig": "*",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-bundle-visualizer": "^0.4.2"
  }
}
