{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn services:up && NODE_ENV=development nodemon src/v2/infra/index.ts",
    "services:up": "docker-compose -f ./src/v2/infra/container/docker-compose.yml up -d",
    "services:down": "docker-compose -f ./src/v2/infra/container/docker-compose.yml stop",
    "build": "tsc",
    "prepare:infra": "kill-port -p 3000 27017",
    "start": "NODE_ENV=production node dist"
  },
  "dependencies": {
    "@fastify/swagger": "^8.2.1",
    "@fastify/swagger-ui": "^1.3.0",
    "argon2": "^0.30.2",
    "dotenv": "^16.0.3",
    "fastify": "^4.10.2",
    "fastify-swagger": "^5.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.2",
    "validate": "*"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^17.0.12",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "kill-port-process": "^3.1.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  }
}
